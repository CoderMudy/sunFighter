    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sys_log_id AS sysLogId, module_name AS moduleName, class_name AS className, ip, msg, params, user_name AS userName, log_type AS logType, times, create_id AS createId, update_id AS updateId, create_date AS createDate, update_date AS updateDate, remark
